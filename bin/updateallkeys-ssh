ssh root@sleepy "cat ~/.ssh/id_rsa.pub" | ssh admin@sleepy "cat >> ~/.ssh/authorized_keys"
ssh admin@sleepy "cat ~/.ssh/id_rsa.pub" | ssh joe@sneezy "cat >> ~/.ssh/authorized_keys"
ssh joe@sneezy "cat ~/.ssh/id_rsa.pub" | ssh admin@sleepy "cat >> ~/.ssh/authorized_keys"
ssh admin@sleepy "cat ~/.ssh/id_rsa.pub" | ssh root@sneezy "cat >> ~/.ssh/authorized_keys"
ssh root@sneezy "cat ~/.ssh/id_rsa.pub" | ssh admin@sleepy "cat >> ~/.ssh/authorized_keys"
ssh admin@sleepy "cat ~/.ssh/id_rsa.pub" | ssh joe@bram.net "cat >> ~/.ssh/authorized_keys"
ssh joe@bram.net "cat ~/.ssh/id_rsa.pub" | ssh admin@sleepy "cat >> ~/.ssh/authorized_keys"
ssh admin@sleepy "cat ~/.ssh/id_rsa.pub" | ssh bramcom@bram.net "cat >> ~/.ssh/authorized_keys"
ssh bramcom@bram.net "cat ~/.ssh/id_rsa.pub" | ssh admin@sleepy "cat >> ~/.ssh/authorized_keys"
#
# Update root@sleepy ssh keys
#
ssh root@sleepy "cat ~/.ssh/id_rsa.pub" | ssh joe@sneezy "cat >> ~/.ssh/authorized_keys"
ssh joe@sneezy "cat ~/.ssh/id_rsa.pub" | ssh root@sleepy "cat >> ~/.ssh/authorized_keys"
ssh root@sleepy "cat ~/.ssh/id_rsa.pub" | ssh root@sneezy "cat >> ~/.ssh/authorized_keys"
ssh root@sneezy "cat ~/.ssh/id_rsa.pub" | ssh root@sleepy "cat >> ~/.ssh/authorized_keys"
ssh root@sleepy "cat ~/.ssh/id_rsa.pub" | ssh joe@bram.net "cat >> ~/.ssh/authorized_keys"
ssh joe@bram.net "cat ~/.ssh/id_rsa.pub" | ssh root@sleepy "cat >> ~/.ssh/authorized_keys"
ssh root@sleepy "cat ~/.ssh/id_rsa.pub" | ssh bramcom@bram.net "cat >> ~/.ssh/authorized_keys"
ssh bramcom@bram.net "cat ~/.ssh/id_rsa.pub" | ssh root@sleepy "cat >> ~/.ssh/authorized_keys"
#
# Update joe@sneezy ssh keys
#
ssh joe@sneezy "cat ~/.ssh/id_rsa.pub" | ssh root@sneezy "cat >> ~/.ssh/authorized_keys"
ssh root@sneezy "cat ~/.ssh/id_rsa.pub" | ssh joe@sneezy "cat >> ~/.ssh/authorized_keys"
ssh joe@sneezy "cat ~/.ssh/id_rsa.pub" | ssh joe@bram.net "cat >> ~/.ssh/authorized_keys"
ssh joe@bram.net "cat ~/.ssh/id_rsa.pub" | ssh joe@sneezy "cat >> ~/.ssh/authorized_keys"
ssh joe@sneezy "cat ~/.ssh/id_rsa.pub" | ssh bramcom@bram.net "cat >> ~/.ssh/authorized_keys"
ssh bramcom@bram.net "cat ~/.ssh/id_rsa.pub" | ssh joe@sneezy "cat >> ~/.ssh/authorized_keys"
