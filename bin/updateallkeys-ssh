# Update joe@bram.net ssh keys
#
ssh joe@bram.net "cat ~/.ssh/id_rsa.pub" | ssh bramcom@bram.net "cat >> ~/.ssh/authorized_keys"
ssh bramcom@bram.net "cat ~/.ssh/id_rsa.pub" | ssh joe@bram.net "cat >> ~/.ssh/authorized_keys"
