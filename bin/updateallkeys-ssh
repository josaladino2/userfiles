#!/bin/bash
#
# Update joe@doc ssh keys
#
ssh joe@doc "cat ~/.ssh/id_rsa.pub" | ssh root@doc "cat >> ~/.ssh/authorized_keys"
ssh root@doc "cat ~/.ssh/id_rsa.pub" | ssh joe@doc "cat >> ~/.ssh/authorized_keys"
ssh joe@doc "cat ~/.ssh/id_rsa.pub" | ssh admin@doc "cat >> ~/.ssh/authorized_keys"
ssh admin@doc "cat ~/.ssh/id_rsa.pub" | ssh joe@doc "cat >> ~/.ssh/authorized_keys"
ssh joe@doc "cat ~/.ssh/id_rsa.pub" | ssh joe@sleepy "cat >> ~/.ssh/authorized_keys"
ssh joe@sleepy "cat ~/.ssh/id_rsa.pub" | ssh joe@doc "cat >> ~/.ssh/authorized_keys"
ssh joe@doc "cat ~/.ssh/id_rsa.pub" | ssh admin@sleepy "cat >> ~/.ssh/authorized_keys"
ssh admin@sleepy "cat ~/.ssh/id_rsa.pub" | ssh joe@doc "cat >> ~/.ssh/authorized_keys"
ssh joe@doc "cat ~/.ssh/id_rsa.pub" | ssh root@sleepy "cat >> ~/.ssh/authorized_keys"
ssh root@sleepy "cat ~/.ssh/id_rsa.pub" | ssh joe@doc "cat >> ~/.ssh/authorized_keys"
ssh joe@doc "cat ~/.ssh/id_rsa.pub" | ssh joe@sneezy "cat >> ~/.ssh/authorized_keys"
ssh joe@sneezy "cat ~/.ssh/id_rsa.pub" | ssh joe@doc "cat >> ~/.ssh/authorized_keys"
ssh joe@doc "cat ~/.ssh/id_rsa.pub" | ssh root@sneezy "cat >> ~/.ssh/authorized_keys"
ssh root@sneezy "cat ~/.ssh/id_rsa.pub" | ssh joe@doc "cat >> ~/.ssh/authorized_keys"
ssh joe@doc "cat ~/.ssh/id_rsa.pub" | ssh cissa@bash "cat >> ~/.ssh/authorized_keys"
ssh cissa@bashful "cat ~/.ssh/id_rsa.pub" | joe@doc "cat >> ~/.ssh/authorized_keys"
ssh joe@doc "cat ~/.ssh/id_rsa.pub" | ssh joe@bram.net "cat >> ~/.ssh/authorized_keys"
ssh joe@bram.net "cat ~/.ssh/id_rsa.pub" | ssh joe@doc "cat >> ~/.ssh/authorized_keys"
ssh joe@doc "cat ~/.ssh/id_rsa.pub" | ssh bramcom@bram.net "cat >> ~/.ssh/authorized_keys"
ssh bramcom@bram.net "cat ~/.ssh/id_rsa.pub" | ssh joe@doc "cat >> ~/.ssh/authorized_keys"
#
# Update admin@doc ssh keys
#
ssh admin@doc "cat ~/.ssh/id_rsa.pub" | ssh root@doc "cat >> ~/.ssh/authorized_keys"
ssh root@doc "cat ~/.ssh/id_rsa.pub" | ssh admin@doc "cat >> ~/.ssh/authorized_keys"
ssh admin@doc "cat ~/.ssh/id_rsa.pub" | ssh joe@sleepy "cat >> ~/.ssh/authorized_keys"
ssh joe@sleepy "cat ~/.ssh/id_rsa.pub" | ssh admin@doc "cat >> ~/.ssh/authorized_keys"
ssh admin@doc "cat ~/.ssh/id_rsa.pub" | ssh admin@sleepy "cat >> ~/.ssh/authorized_keys"
ssh admin@sleepy "cat ~/.ssh/id_rsa.pub" | ssh admin@doc "cat >> ~/.ssh/authorized_keys"
ssh admin@doc "cat ~/.ssh/id_rsa.pub" | ssh root@sleepy "cat >> ~/.ssh/authorized_keys"
ssh root@sleepy "cat ~/.ssh/id_rsa.pub" | ssh admin@doc "cat >> ~/.ssh/authorized_keys"
ssh admin@doc "cat ~/.ssh/id_rsa.pub" | ssh joe@sneezy "cat >> ~/.ssh/authorized_keys"
ssh joe@sneezy "cat ~/.ssh/id_rsa.pub" | ssh admin@doc "cat >> ~/.ssh/authorized_keys"
ssh admin@doc "cat ~/.ssh/id_rsa.pub" | ssh root@sneezy "cat >> ~/.ssh/authorized_keys"
ssh root@sneezy "cat ~/.ssh/id_rsa.pub" | ssh admin@doc "cat >> ~/.ssh/authorized_keys"
ssh admin@doc "cat ~/.ssh/id_rsa.pub" | ssh joe@bram.net "cat >> ~/.ssh/authorized_keys"
ssh joe@bram.net "cat ~/.ssh/id_rsa.pub" | ssh admin@doc "cat >> ~/.ssh/authorized_keys"
ssh admin@doc "cat ~/.ssh/id_rsa.pub" | ssh bramcom@bram.net "cat >> ~/.ssh/authorized_keys"
ssh bramcom@bram.net "cat ~/.ssh/id_rsa.pub" | ssh admin@doc "cat >> ~/.ssh/authorized_keys"
#
# Update root@doc ssh keys
#
ssh root@doc "cat ~/.ssh/id_rsa.pub" | ssh joe@sleepy "cat >> ~/.ssh/authorized_keys"
ssh joe@sleepy "cat ~/.ssh/id_rsa.pub" | ssh root@doc "cat >> ~/.ssh/authorized_keys"
ssh root@doc "cat ~/.ssh/id_rsa.pub" | ssh admin@sleepy "cat >> ~/.ssh/authorized_keys"
ssh admin@sleepy "cat ~/.ssh/id_rsa.pub" | ssh root@doc "cat >> ~/.ssh/authorized_keys"
ssh root@doc "cat ~/.ssh/id_rsa.pub" | ssh root@sleepy "cat >> ~/.ssh/authorized_keys"
ssh root@sleepy "cat ~/.ssh/id_rsa.pub" | ssh root@doc "cat >> ~/.ssh/authorized_keys"
ssh root@doc "cat ~/.ssh/id_rsa.pub" | ssh joe@sneezy "cat >> ~/.ssh/authorized_keys"
ssh joe@sneezy "cat ~/.ssh/id_rsa.pub" | ssh root@doc "cat >> ~/.ssh/authorized_keys"
ssh root@doc "cat ~/.ssh/id_rsa.pub" | ssh root@sneezy "cat >> ~/.ssh/authorized_keys"
ssh root@sneezy "cat ~/.ssh/id_rsa.pub" | ssh root@doc "cat >> ~/.ssh/authorized_keys"
ssh root@doc "cat ~/.ssh/id_rsa.pub" | ssh joe@bram.net "cat >> ~/.ssh/authorized_keys"
ssh joe@bram.net "cat ~/.ssh/id_rsa.pub" | ssh root@doc "cat >> ~/.ssh/authorized_keys"
ssh root@doc "cat ~/.ssh/id_rsa.pub" | ssh bramcom@bram.net "cat >> ~/.ssh/authorized_keys"
ssh bramcom@bram.net "cat ~/.ssh/id_rsa.pub" | ssh root@doc "cat >> ~/.ssh/authorized_keys"
#
# Update joe@sleepy ssh keys
#
ssh joe@sleepy "cat ~/.ssh/id_rsa.pub" | ssh admin@sleepy "cat >> ~/.ssh/authorized_keys"
ssh admin@sleepy "cat ~/.ssh/id_rsa.pub" | ssh joe@sleepy "cat >> ~/.ssh/authorized_keys"
ssh joe@sleepy "cat ~/.ssh/id_rsa.pub" | ssh root@sleepy "cat >> ~/.ssh/authorized_keys"
ssh root@sleepy "cat ~/.ssh/id_rsa.pub" | ssh joe@sleepy "cat >> ~/.ssh/authorized_keys"
ssh joe@sleepy "cat ~/.ssh/id_rsa.pub" | ssh joe@sneezy "cat >> ~/.ssh/authorized_keys"
ssh joe@sneezy "cat ~/.ssh/id_rsa.pub" | ssh joe@sleepy "cat >> ~/.ssh/authorized_keys"
ssh joe@sleepy "cat ~/.ssh/id_rsa.pub" | ssh root@sneezy "cat >> ~/.ssh/authorized_keys"
ssh root@sneezy "cat ~/.ssh/id_rsa.pub" | ssh joe@sleepy "cat >> ~/.ssh/authorized_keys"
ssh joe@sleepy "cat ~/.ssh/id_rsa.pub" | ssh joe@bram.net "cat >> ~/.ssh/authorized_keys"
ssh joe@bram.net "cat ~/.ssh/id_rsa.pub" | ssh joe@sleepy "cat >> ~/.ssh/authorized_keys"
ssh joe@sleepy "cat ~/.ssh/id_rsa.pub" | ssh bramcom@bram.net "cat >> ~/.ssh/authorized_keys"
