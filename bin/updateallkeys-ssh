ssh joe@bram.net "cat ~/.ssh/id_rsa.pub" | ssh root@sneezy "cat >> ~/.ssh/authorized_keys"
ssh root@sneezy "cat ~/.ssh/id_rsa.pub" | ssh bramcom@bram.net "cat >> ~/.ssh/authorized_keys"
ssh bramcom@bram.net "cat ~/.ssh/id_rsa.pub" | ssh root@sneezy "cat >> ~/.ssh/authorized_keys"
