f_bold () {
	printf "${bold_s}$*${no_bold_s}"
}

f_dim () {
	printf "${dim_s}$*${no_dim_s}"
}

f_italic () {
	printf "${italic_s}$*${no_italic_s}"
}

f_underline () {
	printf "${underline_s}$*${no_underline_s}"
}

f_blink () {
	printf "${blink_s}$*${no_blink_s}"
}

f_reverse () {
	printf "${reverse_s}$*${no_reverse_s}"
}

f_invisible () {
	printf "${invisible_s}$*${no_invisible_s}"
}

